<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Fdefault"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    tools:context=".Fragmentos.FragmentDefault">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="275dp"
        android:layout_height="278dp"
        android:src="@drawable/user2"
        app:layout_constraintBottom_toTopOf="@+id/Nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.617" />

    <TextView
        android:id="@+id/Nombre"
        android:layout_width="335dp"
        android:layout_height="37dp"
        android:layout_marginBottom="96dp"
        android:gravity="center_horizontal"
        android:text="Jimenez Cruz Ricardo"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/carrera"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView

        android:id="@+id/carrera"
        android:layout_width="359dp"
        android:layout_height="144dp"
        android:layout_marginBottom="64dp"
        android:gravity="center_vertical"
        android:text="Ingeniria en Sistemas Computacionales"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>